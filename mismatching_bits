#include <stdio.h>

//int A = 10, int B = 11

//1010 //1011
int main(){
  int a = 4;
  int b = 7;
  int i = 0;
  int count = 0;
  while( i<=30){
      //printf("%d ",a&(1<<i));
      //printf("%d ",b&(1<<i));
    if((a&(1<<i)) != (b&(1<<i))){
      count++;
    }
    i++;
  }
  printf("%d ",count);

  return 0;
}
